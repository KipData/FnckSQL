# F051-04: Comparison predicate on DATE, TIME, and TIMESTAMP data types

query B
SELECT DATE '2016-03-26' < DATE '2016-03-26'
----
false

query B
SELECT DATE '2016-03-26' <= DATE '2016-03-26'
----
true

query B
SELECT DATE '2016-03-26' <> DATE '2016-03-26'
----
false

query B
SELECT DATE '2016-03-26' = DATE '2016-03-26'
----
true

query B
SELECT DATE '2016-03-26' > DATE '2016-03-26'
----
false

query B
SELECT DATE '2016-03-26' >= DATE '2016-03-26'
----
true

